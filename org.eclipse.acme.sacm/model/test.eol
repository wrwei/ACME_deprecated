operation createGoal(name: String, description: String) {
	var goal = new Goal;
	var n = new LangString;
	n.content = name;
	var d = new Description;
	var content = new MultiLangString;
	var value = new LangString;
	value.content = description;
	d.content = content;
	content.value.add(value);
	//createName(goal, name);
	goal.name = n;
	goal.description = d;
	return goal;
}

operation createName(element, name){
	var n = new LangString;
	n.content = name;
	element.name = n;
}

operation createDescription(element, description){
	var d = new Description;
	var content = new MultiLangString;
	d.content = content;
}

operation createModule(name: String, description: String) {
	var goal = new Module;
	var n = new LangString;
	n.content = name;
	var d = new Description;
	var content = new MultiLangString;
	var value = new LangString;
	value.content = description;
	
	d.content = content;
	content.value.add(value);
	goal.name = n;
	goal.description = d;
	return goal;
}