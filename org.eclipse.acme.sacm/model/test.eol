var g1 = createGoal('N', 'D');
var m1 = createModule('NN', 'DD');
m1.argumentationElement.add(g1);


operation createGoal(name: String, description: String) {
	var goal = new Goal;
	var n = new LangString;
	n.content = name;
	var d = new Description;
	var content = new MultiLangString;
	var value = new LangString;
	value.content = description;
	
	d.content = content;
	content.value.add(value);
	goal.name = n;
	goal.description = d;
	return goal;
}

operation createModule(name: String, description: String) {
	var goal = new Module;
	var n = new LangString;
	n.content = name;
	var d = new Description;
	var content = new MultiLangString;
	var value = new LangString;
	value.content = description;
	
	d.content = content;
	content.value.add(value);
	goal.name = n;
	goal.description = d;
	return goal;
}